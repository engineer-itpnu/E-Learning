{% extends '::base.html.twig' %}

{% block body -%}
    <h1>university</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ entity.phone }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ entity.website }}</td>
            </tr>
            <tr>
                <th>Enddate</th>
                <td>{{ entity.enddate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Regdate</th>
                <td>{{ entity.regdate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('university') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('university_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
